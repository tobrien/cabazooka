{
    "name": "@tobrien/cabazooka",
    "version": "0.0.1",
    "description": "cabazooka doesn't help you think. It removes what gets in the way.",
    "main": "dist/cabazooka.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tobrien/cabazooka.git"
    },
    "packageManager": "pnpm@10.8.1",
    "exports": {
        ".": {
            "import": "./dist/cabazooka.js",
            "types": "./dist/cabazooka.d.ts"
        }
    },
    "scripts": {
        "build": "vite build",
        "start": "dist/main.js",
        "dev": "vite",
        "watch": "vite build --watch",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build"
    },
    "keywords": [
        "focus",
        "productivity",
        "notes",
        "genai"
    ],
    "author": "Tim O'Brien <tobrien@discursive.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "@tobrien/minorprompt": "^0.0.2",
        "@types/fluent-ffmpeg": "^2.1.27",
        "commander": "^13.1.0",
        "dayjs": "^1.11.13",
        "dotenv": "^16.4.7",
        "fluent-ffmpeg": "^2.1.3",
        "glob": "^11.0.1",
        "js-yaml": "^4.1.0",
        "luxon": "^3.5.0",
        "moment-timezone": "^0.5.48",
        "openai": "^4.91.1",
        "winston": "^3.17.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@swc/core": "^1.4.8",
        "@types/jest": "^29.5.14",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.6.2",
        "@types/node": "^22.13.14",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "eslint": "^9.24.0",
        "eslint-plugin-import": "^2.31.0",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "mockdate": "^3.0.5",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "ts-jest": "^29.2.6",
        "typescript": "^5.3.3",
        "vite": "^6.2.5",
        "vite-plugin-dts": "^4.5.3",
        "vite-plugin-node": "^5.0.0"
    },
    "pnpm": {
        "overrides": {
            "wrap-ansi": "7.0.0"
        }
    }
}